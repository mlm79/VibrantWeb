{
  "name": "Vibrant Web",
  "version": "1.0",
  "description": "Mining and visualizing 3rd-party .",
  "permissions": [
    "tabs", "http://*/*", "https://*/*"
  ],
  "background_page": "background.html",
  "content_scripts": [
    {
      "matches": ["http://*/*"],
      "css": ["vibrant.css"],
      "js": ["jquery-1.6.4.min.js","jstorage.js","utils.js","vibrant.js"]
    }
  ],
  "browser_action": {
    "default_title": "VibrantWeb",    // optional; shown in tooltip
    "default_icon": "icon.png",
    "popup": "popup.html"
  }
}