{
  "name": "Vibrant Web",
  "version": "1.0",
  "description": "Mining and visualizing 3rd-party .",
  "permissions": [
    "http://*/*", "https://*/*","tabs"
  ],
  "background_page": "background.html",
  "content_scripts": [
    {
      "matches": ["http://*/*"],
      "js": ["js/jquery-1.6.4.min.js","js/jstorage.js","js/utils.js","js/vibrant.js"]
    }
  ],
  "browser_action": {
    "default_title": "VibrantWeb",  
    "default_icon": "imgs/icon.png",
    "popup": "popup.html"
  }
}

